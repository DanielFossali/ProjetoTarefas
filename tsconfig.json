{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "20.1.0",

      "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true
      },
      "include": ["src/**/*"],
      "exclude": ["node_modules"]
    }